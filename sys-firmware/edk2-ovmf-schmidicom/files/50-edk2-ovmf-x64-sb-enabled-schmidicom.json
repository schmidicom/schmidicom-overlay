{
    "description": "OVMF for x86_64, with SB+SMM, SB Enabled, compiled by schmidicom",
    "interface-types": [
        "uefi"
    ],
    "mapping": {
        "device": "flash",
        "executable": {
            "filename": "/usr/share/edk2-ovmf-schmidicom/OVMF_CODE.fd",
            "format": "raw"
        },
        "nvram-template": {
            "filename": "/usr/share/edk2-ovmf-schmidicom/OVMF_VARS_sb-enabled.fd",
            "format": "raw"
        }
    },
    "targets": [
        {
            "architecture": "x86_64",
            "machines": [
                "pc-q35-*"
            ]
        }
    ],
    "features": [
        "acpi-s3",
        "acpi-s4",
        "amd-sev",
        "requires-smm",
        "secure-boot"
    ],
    "tags": [

    ]
}
